<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:viewModels="clr-namespace:QuantTrader.ViewModels.Sidebar"
             xmlns:sidebar="clr-namespace:QuantTrader.Views.Sidebar"
             mc:Ignorable="d" d:DesignWidth="250" d:DesignHeight="600"
             x:Class="QuantTrader.Views.Sidebar.SidebarView">
    <Design.DataContext>
        <viewModels:SidebarViewModel/>
    </Design.DataContext>
    
    <sidebar:SidebarNavView DataContext="{Binding SidebarNavViewModel}"/>
</UserControl>
